---
import { Image } from 'astro:assets'

import Layout from '../layouts/Layout.astro'

import LetterPullup from '../components/LetterPullup'
import NumberTicker from '../components/NumberTicker'

import carlosImage from '../assets/carlos.png'
import mybitgImage from '../assets/mybitg.png'
import psybitcoinImage from '../assets/psybitcoin.png'
import psyuiImage from '../assets/psyui.png'

type Tik = {
  t: number
  p: number
}

type BitboResponse = {
  '1_min': Tik[]
}

const response = await fetch(
  'https://api.bitbo.io/price-history?interval=1_min&limit=2',
  { headers: { 'Cache-Control': 'no-cache' } }
)
const data: BitboResponse = (await response.json()) as BitboResponse
const priceUSD = data['1_min'][1].p
